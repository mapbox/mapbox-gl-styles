    "source-layer": "road",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "street"]],
    "layout": {
      "visibility": "visible",
      "line-cap": "round"
    },
    "paint": {
      "line-width": "@street-width",
      "line-color": "#cfc8c0"
    }
  }, {
    "id": "road-main",
    "type": "line",
    "source": "mapbox://mapbox.mapbox-streets-v6",
    "source-layer": "road",
    "minzoom": 0,
    "maxzoom": 22,
