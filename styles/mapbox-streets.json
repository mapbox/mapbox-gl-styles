    "@landuse-sand": "#f0dfaf",
    "@motorway-trunk-case-width": {
      "base": 1.4,
      "stops": [[6.5, 0], [7.5, 2.5], [18, 34]]
    },
    "@water": "#73b6e6",
    "@label-state": "#242424",
    "@landuse-parks": "#bdd98c",
    "@label-road": "#3a3836",
    "@label-transit": "red",
    "@label-secondary": "#936d49",
    "@halo": "#ffffff",
    "@building-extras": "#d2cbc3",
