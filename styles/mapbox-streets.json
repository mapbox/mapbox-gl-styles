  }, {
    "id": "bridge-motorway",
    "type": "line",
    "source": "mapbox://mapbox.mapbox-streets-v6",
    "source-layer": "bridge",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "motorway"]],
    "layout": {
      "visibility": "visible",
      "line-cap": "round"
    },
    "paint": {
      "line-width": "@motorway-width",
      "line-color": "@road-motorway"
    }
  }, {
    "id": "admin-3-4-boundaries",
    "type": "line",
    "source": "mapbox://mapbox.mapbox-streets-v6",
    "source-layer": "admin",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", [">=", "admin_level", 3], ["==", "maritime", 0]],
    "layout": {
      "visibility": "visible",
      "line-join": "bevel"
    },
    "paint": {
      "line-color": "#686868",
      "line-width": {
        "base": 1.3,
        "stops": [[3, 1], [14, 2]]
      },
      "line-opacity": {
        "base": 1,
        "stops": [[2, 0], [5, 1]]
      },
      "line-dasharray": [4, 1]
