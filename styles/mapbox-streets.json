    "id": "background",
    "type": "background",
    "minzoom": 0,
    "maxzoom": 22,
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "background-color": "@background"
    }
  }, {
    "id": "landcover_crop",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-terrain-v2",
    "source-layer": "landcover",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "crop"]],
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "fill-color": "@landcover",
      "fill-opacity": "@landcover-opacity",
      "fill-antialias": false
    }
  }, {
    "id": "landcover_grass",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-terrain-v2",
    "source-layer": "landcover",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "grass"]],
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "fill-color": "@landcover",
      "fill-opacity": "@landcover-opacity",
      "fill-antialias": false
    }
  }, {
    "id": "landcover_scrub",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-terrain-v2",
    "source-layer": "landcover",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "scrub"]],
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "fill-color": "@landcover",
      "fill-opacity": "@landcover-opacity",
      "fill-antialias": false
    }
  }, {
    "id": "landcover_wood",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-terrain-v2",
    "source-layer": "landcover",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "wood"]],
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "fill-color": "@landcover",
      "fill-opacity": "@landcover-opacity",
      "fill-antialias": false
    }
  }, {
    "id": "landcover_snow",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-terrain-v2",
    "source-layer": "landcover",
