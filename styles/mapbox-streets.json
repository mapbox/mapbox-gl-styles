    "source": "mapbox://mapbox.mapbox-streets-v6",
    "source-layer": "road",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["all", ["==", "class", "main"], ["!=", "type", "trunk"]],
    "layout": {
      "visibility": "visible",
      "line-cap": "round"
    },
    "paint": {
      "line-width": "@main-case-width",
      "line-color": "@road-case-dark"
    }
  }, {
    "id": "road-motorway-trunk-case",
    "type": "line",
    "source": "mapbox://mapbox.mapbox-streets-v6",
    "source-layer": "road",
    "minzoom": 0,
    "maxzoom": 22,
    "filter": ["any", ["in", "class", "motorway"], ["==", "type", "trunk"]],
    "layout": {
      "visibility": "visible",
      "line-cap": "round"
    },
    "paint": {
      "line-width": "@motorway-trunk-case-width",
      "line-color": "@road-case"
