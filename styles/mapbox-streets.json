    "maxzoom": 22,
    "filter": ["all"],
    "layout": {},
    "paint": {
      "line-color": "@building-extras"
    }
  }, {
    "id": "water",
    "type": "fill",
    "source": "mapbox://mapbox.mapbox-streets-v6",
